--[[ This is obviously going to look very shitty for it is just an example ]]--

-- Define a namespace for our UI library
local MyUI = {}

-- Define a function to create a new frame
function MyUI.newFrame(name, parent, size, position)
    local frame = Instance.new("Frame")
    frame.Name = name
    frame.Parent = parent
    frame.Size = size
    frame.Position = position
    return frame
end

-- Define a function to create a new button
function MyUI.newButton(name, parent, size, position, text)
    local button = Instance.new("TextButton")
    button.Name = name
    button.Parent = parent
    button.Size = size
    button.Position = position
    button.Text = text
    return button
end

-- Define a function to create a new label
function MyUI.newLabel(name, parent, size, position, text)
    local label = Instance.new("TextLabel")
    label.Name = name
    label.Parent = parent
    label.Size = size
    label.Position = position
    label.Text = text
    return label
end

-- Export the namespace
return MyUI
